{
    "breadcrumbs.enabled": true,
    "css.lint.hexColorLength": "ignore",
    "css.lint.important": "warning",
    "editor.fontFamily": "'Hack Nerd Font', 'Hack', 'Fira Mono', Menlo Bold, Monaco, 'Courier New', monospace",
    "editor.minimap.enabled": false,
    "editor.formatOnSave": false,
    "editor.autoClosingBrackets": "always",
    "editor.padding.top": 20,
    "editor.padding.bottom": 20,
    "editor.inlineSuggest.enabled": true,
    "editor.guides.bracketPairs": "active",
    "editor.scrollbar.horizontal": "hidden",
    "editor.formatOnPaste": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.wordWrap": "on",
    "editor.autoIndent": "advanced",
    "editor.find.addExtraSpaceOnTop": false,
    "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss06', 'zero'",
    "editor.codeActionsOnSave": {
        "source.fixAll": false,
        "source.fixAll.eslint": true,
        "source.organizeImports": false
    },
    "eslint.codeAction.showDocumentation": {
        "enable": true
    },
    "eslint.quiet": true,
    "eslint.validate": [
        "javascript",
        "typescript",
        "javascriptreact",
        "typescriptreact",
        "vue",
        "html",
        "markdown",
        "json",
        "jsonc",
        "json5"
    ],
    "explorer.autoReveal": true,
    "files.autoSave": "off",
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
        ".git": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.vscode": true,
        "**/.hg": true,
        "**/.pyc": true,
        "**/.map": true,
        "*.pyc": true,
        "*.map": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        ".DS_Store": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/bower_components/*/**": true
    },
    "github.copilot.enable": {
        "*": true,
        "markdown": true,
        "plaintext": false,
        "yaml": false
    },
    "gitlens.codeLens.authors.enabled": true,
    "gitlens.codeLens.enabled": true,
    "gitlens.gitCommands.search.showResultsInSideBar": false,
    "html.format.enable": true,
    "html.format.endWithNewline": false,
    "intelephense.stubs": [
        "apache",
        "bcmath",
        "bz2",
        "calendar",
        "com_dotnet",
        "Core",
        "ctype",
        "curl",
        "date",
        "dba",
        "dom",
        "enchant",
        "exif",
        "fileinfo",
        "filter",
        "fpm",
        "ftp",
        "gd",
        "hash",
        "iconv",
        "imap",
        "interbase",
        "intl",
        "json",
        "ldap",
        "libxml",
        "mbstring",
        "mcrypt",
        "meta",
        "mssql",
        "mysqli",
        "oci8",
        "odbc",
        "openssl",
        "pcntl",
        "pcre",
        "PDO",
        "pdo_ibm",
        "pdo_mysql",
        "pdo_pgsql",
        "pdo_sqlite",
        "pgsql",
        "Phar",
        "posix",
        "pspell",
        "readline",
        "recode",
        "Reflection",
        "regex",
        "session",
        "shmop",
        "SimpleXML",
        "snmp",
        "soap",
        "sockets",
        "sodium",
        "SPL",
        "sqlite3",
        "standard",
        "superglobals",
        "sybase",
        "sysvmsg",
        "sysvsem",
        "sysvshm",
        "tidy",
        "tokenizer",
        "wddx",
        "xml",
        "xmlreader",
        "xmlrpc",
        "xmlwriter",
        "Zend OPcache",
        "zip",
        "zlib"
    ],
    "intelephense.telemetry.enabled": false,
    "intelephense.format.enable": true,
    "intelephense.completion.triggerParameterHints": true,
    "intelephense.completion.insertUseDeclaration": true,
    "intelephense.completion.fullyQualifyGlobalConstantsAndFunctions": true,
    "intelephense.trace.server": "messages",
    "intelephense.files.exclude": [
        "**/.git/**",
        "**/.svn/**",
        "**/.hg/**",
        "**/CVS/**",
        "**/.DS_Store/**",
        "**/node_modules/**",
        "**/bower_components/**",
        "**/storage",
        "**/storage/**",
        "**/tests/**",
        "**/resources/views/**",
        "**/database/migrations/**",
        "**/storage/framework/views/**",
        "_ide_helper.php",
        "_ide_helper_models"
    ],
    "markdown.preview.scrollEditorWithPreview": true,
    "markdown.preview.scrollPreviewWithEditor": true,
    "prettier.enable": false,
    "scss.lint.important": "warning",
    "search.exclude": {
        "**/.git": true,
        "**/.github": true,
        "**/.nuxt": true,
        "**/.output": true,
        "**/.pnpm": true,
        "**/.vscode": true,
        "**/.yarn": true,
        "**/bower_components": true,
        "**/dist/**": true,
        "**/logs": true,
        "**/node_modules": true,
        "**/out/**": true,
        "**/package-lock.json": true,
        "**/pnpm-lock.yaml": true,
        "**/tmp": true,
        "**/yarn.lock": true
    },
    "workbench.iconTheme": "vscode-icons",
    "workbench.editor.tabCloseButton": "right",
    "workbench.editor.closeOnFileDelete": false,
    "workbench.colorTheme": "Monokai Pro New KC",
    "workbench.sideBar.location": "right",
    "workbench.settings.editor": "json",
    "workbench.fontAliasing": "antialiased",
    "solidity.telemetry": false,
    "telemetry.telemetryLevel": "off",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.external.osxExec": "iTerm.app",
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.editor.titleScrollbarSizing": "large",
    "editor.acceptSuggestionOnEnter": "off",
    "editor.quickSuggestionsDelay": 50,
    "window.closeWhenEmpty": true,
    "editor.dragAndDrop": false,
    "python.defaultInterpreterPath": "/usr/local/bin/python3",
    "explorer.confirmDragAndDrop": false,
}